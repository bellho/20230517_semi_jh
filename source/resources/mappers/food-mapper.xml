<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="foodMapper1">
	<select id="selectListFood" resultType="ProcessecFoodDto" parameterType="string">
		SELECT food_name,food_code,manufacturer,food_date,food_db_category,food_category,information_source,calories tb_processed_food 
		where food_name like '%'||#{keyword}||'%'
	</select>
</mapper>
